<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lcc.videograph" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".TitleScreenActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" >
        </activity>
        <activity
            android:name=".PlotActivity"
            android:label="VideoGraph"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name=".ScaleActivity"
            android:label="@string/title_activity_scale" >
        </activity>
        <activity
            android:name=".GraphingActivity"
            android:label="@string/title_activity_graph" >
        </activity>
        <activity
            android:name=".Guides2Activity"
            android:label="GraphingActivity" >
        </activity>
        <activity
            android:name=".ScaleVideo"
            android:label="ScaleVideo" >
        </activity>
        <activity
            android:name=".RollingMaterialsActivity"
            android:label="@string/title_activity_rolling_materials" >
        </activity>
        <activity
            android:name=".RollingSetupActivity"
            android:label="@string/title_activity_rolling_setup" >
        </activity>
        <activity
            android:name=".KineticMaterialsActivity"
            android:label="@string/title_activity_kinetic_materials" >
        </activity>
    </application>

</manifest>
